    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "request": "^2.71.0",
    "rimraf": "^2.5.2",
    "walk": "^2.3.9",
    "xml-parser": "^1.2.1",
    "yazl": "^2.3.0"
  }
